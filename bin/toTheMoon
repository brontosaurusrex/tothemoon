#!/bin/bash

# colect info from multiple machines, ssh key auth must be working first.

errcount=0

# myself, angela
$HOME/bin/nvidiaInfoOnce
(( $? != 0 )) && let errcount++

# melania
ssh rudi@192.168.0.101 "~/bin/nvidiaInfoOnce"
(( $? != 0 )) && let errcount++

# this is parsed by toTheMoonWrap:
echo "errcount $errcount"
